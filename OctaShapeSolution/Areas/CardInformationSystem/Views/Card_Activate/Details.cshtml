@model IEnumerable<OctaShape.Data.Card_ReceivedDetails>

@{
    ViewBag.Title = "Card Information System - Activation List ";
    Layout = "~/Areas/CardInformationSystem/Views/Shared/_AdminLayout.cshtml";
}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Card Activated On : ---</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>

    
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Activation_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Branch_Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Account_No)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Card_No)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Is_Activated)
            </th>
           
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Activation_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Branch_Code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Account_No)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Card_No)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Is_Activated)
                </td>
             
            </tr>
        }

    </table>
</div>